{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Backend",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-backend",
      "program": "${workspaceFolder}/src/NetWorthNavigator.Backend/bin/Debug/net10.0/NetWorthNavigator.Backend.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/NetWorthNavigator.Backend",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      }
    },
    {
      "name": "Attach to Backend",
      "type": "coreclr",
      "request": "attach"
    }
  ],
  "compounds": []
}
